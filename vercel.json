{
  "version": 2,
  "builds": [
    {
      "src": "api/komentar.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/komentar",
      "methods": ["GET", "POST"],
      "dest": "/api/komentar.js"
    },
    {
      "src": "/api/komentar",
      "methods": ["DELETE"],
      "dest": "/api/komentar.js"
    }
  ]
}
